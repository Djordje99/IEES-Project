        <Label Content="Choose Method" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" RenderTransformOrigin="-1.365,0.159" Width="215"/>
        <Label x:Name="LabelNum2" Content="" HorizontalAlignment="Left" Margin="10,73,0,0" VerticalAlignment="Top" RenderTransformOrigin="-1.365,0.159" Width="215"/>
